    <description>Screen flow, community pages. Sign a guest waiver. A Guest Pass Id can be passed in to link the waiver to an existing guest pass.</description>
    <environments>Default</environments>
    <formulas>
        <name>strFmlScreenHeader</name>
        <dataType>String</dataType>
        <expression>&quot;Claim Guest Pass&quot; + IF( !ISBLANK( {!Get_Guest_Pass.Guest_First_Name__c} ), &quot; for &quot; + {!Get_Guest_Pass.Guest_First_Name__c} + &quot; &quot; + {!Get_Guest_Pass.Guest_Last_Name__c}, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>strFmlWaiverName</name>
        <dataType>String</dataType>
        <expression>{!Get_Guest_Pass.Guest_First_Name__c} + &quot; &quot; + {!Get_Guest_Pass.Guest_Last_Name__c}  + &quot; - &quot; + {!Get_Contract_Template.Name}</expression>
    </formulas>
    <formulas>
        <name>strFmlZipCode</name>
        <dataType>String</dataType>
        <expression>IF( !ISBLANK( {!Get_Guest_Pass.Contact__c} ), 
  {!Get_Guest_Pass.Contact__r.MailingPostalCode}, 
  {!Get_Guest_Pass.Lead__r.PostalCode}
)</expression>
    </formulas>
